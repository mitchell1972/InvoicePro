{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/production-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "production-server.js": {
      "maxDuration": 30
    },
    "api/index.js": {
      "maxDuration": 30
    }
  }
}


