{
  "version": 2,
  "builds": [
    {
      "src": "production-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/production-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/production-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "production-server.js": {
      "maxDuration": 10
    }
  }
}


