{
  "version": 2,
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/production-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/production-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "production-server.js": {
      "maxDuration": 30
    }
  }
}


